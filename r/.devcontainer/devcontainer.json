{
    "name": "R playground",
    "dockerComposeFile": [
        "../docker-compose.yml"
    ],
    "service": "playground",
    "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-toolsai.jupyter",
                "REditorSupport.r",
                "RDebugger.r-debugger"
            ]
        }
    },
    "remoteUser": "root"
}
